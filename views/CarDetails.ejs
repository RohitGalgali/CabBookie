        <body>
            <div class="container">
                <div class="col-lg-5 mb-4">
                    <div class="card-body">
                        <h4 class="card-title">Journey Details</h4>
                        <h6 class="card-subtitle mb-3 text-muted" style="text-align: left">Car:
                            <%= CarID%>
                        </h6>
                        <h6 class="card-subtitle mb-3 text-muted">Travel:
                            <%if(Direction == 'a2p')
                            {%>
                                Aurangabad To Pune
                                <%}else{%>
                                    Pune To Aurangabad
                                    <%}%>

                        </h6>
                        <h6 class="card-subtitle mb-3 text-muted">Date:
                            <%= Date%>
                        </h6>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-2 mb-4"></div>
                    <div class="col-lg-1 mb-4">
                        <div class="card h-100">
                            <h4 class="squaredOne">
                                <%if(Dataset.length===0){%>
                                    <input type="checkbox"  value="None" id="squaredOne" name="check" />
                                    <%} else {%>
                                <%for(var entry of Dataset){ %>
                                     <%if(entry.SeatNumber === 1){%>
                                        <input type="checkbox"  value="None" id="squaredOne" name="check" />
                                    <%}else{%>
                                        <input type="checkbox" checked="checked" value="None" id="squaredOne" name="check" />
                                        <%}%>
                                    <% }%>
                                    <%}%>
                                    <label for="squaredOne"></label>
                            </h4>
                        </div>
                    </div>
                    <div class="col-lg-1 mb-4">
                        <div class="card h-100">
                            <h4 class="squaredOne">
